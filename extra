///how to install package 
> open cmd 
> npm i @babel/core @babel/preset-env @babel/register


function addData(att_key,att_value){
    var data = {
        "attributes":[
            {"name":"test1",value:"Aakash"},
            {"name":"test2",value:"Bhumika"}
        ]
    }

    for (mydata of data.attributes){
        if(mydata.name===att_key){
            mydata.value=att_value;
            break;
        }else{
            data.attributes.push({"name":att_key,"value":att_value})
            break;
        }
    }

    return data
}

